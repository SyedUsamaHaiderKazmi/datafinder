{
    "name": "suhk/datafinder-laravel",
    "description": "This is a package to implement search and filters on the backend level using DataTables and Laravel Models. The best part of this package is the ability to search across multiple tables and rows in a single request (ajax), with or without using joins.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Usama Haider Kazmi",
            "email": "usamakazmi.uk@gmail.com",
            "role": "Owner/Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3 <8.5",
        "illuminate/support": "5.8.*|6.*|7.*|8.*|9.*|10.*|11.*"
    },
    "autoload": {
        "psr-4": {
            "SUHK\\DataFinder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SUHK\\DataFinder\\App\\Providers\\MainServiceProvider"
            ],
            "aliases": {
                "DataFinder": "SUHK\\DataFinder\\App\\Facades\\DataFinder"
            }
        }
    }
}
